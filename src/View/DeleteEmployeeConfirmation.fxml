<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.DeleteEmployeeConfirmationController">
   <children>
      <Label fx:id="ConfirmationQuestionLabel" layoutX="48.0" layoutY="105.0" text="Are you sure you want to delete this Employee?" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="NoButton" layoutX="155.0" layoutY="245.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="110.0" onAction="#handleCloseButtonAction" text="No" />
      <Button fx:id="YesButton" layoutX="352.0" layoutY="245.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="110.0" onAction="#deleteUser" text="Yes" />
   </children>
</AnchorPane>
