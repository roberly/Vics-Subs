<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddShiftController">
   <children>
      <TextField fx:id="tfstarttime" layoutX="175.0" layoutY="109.0" promptText="Shift Start Time" />
      <TextField fx:id="tfendtime" layoutX="175.0" layoutY="150.0" promptText="Shift End Time" />
      <Label fx:id="lblemployee" layoutX="186.0" layoutY="55.0" text="Label" />
      <Button fx:id="btnok" layoutX="126.0" layoutY="204.0" mnemonicParsing="false" onAction="#onAddShift" text="Add Shift" />
      <Button fx:id="btncancel" layoutX="223.0" layoutY="204.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
      <Label layoutX="82.0" layoutY="113.0" text="Shift Start Time">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="88.0" layoutY="154.0" text="Shift End Time">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
