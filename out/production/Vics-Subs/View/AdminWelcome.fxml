<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AdminWelcomeController">
   <children>
      <Button fx:id="LogoutButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleCloseButtonAction" text="Logout" />
      <Button fx:id="InputHoursButton" layoutX="241.0" layoutY="126.0" mnemonicParsing="false" onMouseClicked="#bringUpInputHoursScreen" text="Input/Delete Hours" />
      <Button fx:id="ModifyScheduleButton" layoutX="248.0" layoutY="275.0" mnemonicParsing="false" onAction="#bringUpModifySchedule" text="Modify Schedule" />
      <Button fx:id="RequestedDaysOffButton" layoutX="225.0" layoutY="230.0" mnemonicParsing="false" text="View Requested Days Off" />
      <Button fx:id="ModifyEmployeeAccountButton" layoutX="222.0" layoutY="175.0" mnemonicParsing="false" onMouseClicked="#bringUpModifyEmployee" text="Modify Employee Account" />
      <Label layoutX="203.0" layoutY="67.0" text="Welcome to Vic's Admin">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="231.0" layoutY="320.0" mnemonicParsing="false" text="View All Hours Worked" />
   </children>
</AnchorPane>
